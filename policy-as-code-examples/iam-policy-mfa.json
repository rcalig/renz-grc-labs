{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyConsoleLoginWithoutMFA",
      "Effect": "Deny",
      "Action": "sts:AssumeRole",
      "Resource": "*",
      "Condition": {
        "BoolIfExists": {
          "aws:MultiFactorAuthPresent": "false"
        }
      }
    }
  ]
}
